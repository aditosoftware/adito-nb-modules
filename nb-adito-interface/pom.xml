  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.netbeans.utilities</groupId>
        <artifactId>nbm-maven-plugin</artifactId>
        <configuration>
          <publicPackages>
            <publicPackage>de.adito.aditoweb.nbm.nbide.nbaditointerface.*</publicPackage>
            <publicPackage>de.adito.aditoweb.nbm.nbide.nbaditointerface.actionitems.*</publicPackage>
            <publicPackage>de.adito.aditoweb.nbm.nbide.nbaditointerface.common.*</publicPackage>
