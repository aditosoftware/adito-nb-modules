  <build>
    <plugins>
      <!-- NetBeans module plugin. Muss explizit fÃ¼r packaging 'nbm' angegeben werden. -->
      <plugin>
        <groupId>org.apache.netbeans.utilities</groupId>
        <artifactId>nbm-maven-plugin</artifactId>
        <configuration>
          <publicPackages>
            <publicPackage>org.netbeans.modules.javascript.editing.*</publicPackage>
            <publicPackage>org.netbeans.modules.javascript.editing.adito.*</publicPackage>
            <publicPackage>org.netbeans.modules.javascript.editing.embedding.*</publicPackage>
            <publicPackage>org.netbeans.modules.javascript.editing.lexer.*</publicPackage>
            <publicPackage>org.netbeans.modules.javascript.editing.spi.*</publicPackage>
          </publicPackages>
          <codeNameBase>org.netbeans.modules.javascript.editing</codeNameBase>

          <moduleDependencies>
            <dependency>
              <id>org.netbeans.modules:org-netbeans-modules-html-editor</id>
              <type>impl</type>
              <explicitValue>org.netbeans.modules.html.editor/2 = netbeans-release-428-on-20190716</explicitValue>
            </dependency>
            <dependency>
              <id>org.netbeans.modules:org-netbeans-modules-html-editor-lib</id>
