  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>nbm-maven-plugin</artifactId>
      </plugin>

      <!-- kopiert die jsstubs -->
      <plugin>
        <artifactId>maven-resources-plugin</artifactId>
        <version>2.4.3</version>
        <executions>
          <execution>
            <id>copy-resources</id>
            <phase>validate</phase>
            <goals>
              <goal>copy-resources</goal>
            </goals>
            <configuration>
              <useDefaultDelimiters>false</useDefaultDelimiters>
              <overwrite>true</overwrite>
              <outputDirectory>${basedir}/target/nbm/netbeans/aditoonline/jsstubs</outputDirectory>
              <resources>
                <resource>
                  <directory>src/main/nbm/jsstubs</directory>
                  <!--<directory>../../../library/server/target/classes/generated/js/stubs</directory>-->
                </resource>
              </resources>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>


